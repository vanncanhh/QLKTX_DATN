@if (Model != null && Model.PhongModelViews != null && Model.PhongModelViews.Count > 0)
{
        <section class="box-section">
            <div class="content">
                <div class="container">
                    <div class="shoe-product">
                    @foreach (var item in Model.PhongModelViews)
                    {
                        if (!string.IsNullOrEmpty(item.Key))
                        {
                                        <div class="titlehome">
                                <i></i><span class="fa fa-home"></span>
                                            <h2>
                                                <a href="jordan-1-high">@item.Key</a>
                                            </h2>
                                            @*<a href="/san-pham/@item." class="viewall">Xem tất cả</a>*@
                                        </div>
                            foreach (var itemProduct in item.Value.Take(10))
                            {
                                                <div class="product-item">
                                                    <a href="/chi-tiet-phong/@itemProduct.Id">
                                                    <div class="images custom-image-status">
                                            @if (!string.IsNullOrEmpty(itemProduct.TinhTrangStr))
                                            {
                                                <div class="trang-thai-phong"><span>@itemProduct.TinhTrangStr</span></div>
                                            }
                                             @if (!string.IsNullOrEmpty(itemProduct.LoaiPhongStr))
                                            {
                                                <div class="loai-phong"><span>@itemProduct.LoaiPhongStr</span></div>
                                            }                                                                                            
                                            @if (!string.IsNullOrEmpty(itemProduct.HinhAnh))
                                            {
                                                <img width="234" height="234" class=" ls-is-cached lazyloaded" src="/category-image/@itemProduct.HinhAnh">
                                            }else{
                                                <img width="234" height="234" class=" ls-is-cached lazyloaded" src="/image-default/default.png" />
                                            }
                                                    </div>
                                                    <div class="info">
                                                                                        <h3 style="height: unset">
                                                Tên phòng: @itemProduct.TenPhong
                                                        </h3>
                                                        <p>
                                                            <span>Đơn giá</span>
                                                @(
                                                                !string.IsNullOrEmpty(itemProduct.DonGiaStr) ? itemProduct.DonGiaStr : ""
                                                        ) đ<span>
                                                   @* @(
                                                                !string.IsNullOrEmpty(itemProduct.price_reduced_str) ? itemProduct.price_reduced_str : ""
                                                            )*@
                                                            </span>
                                                        </p>
                                                    </div>
                                                </a>
                                            </div>
                            }

                        }
                    }

                    @if (Model.DichVus != null && Model.DichVus.Count > 0)
                    {
                        <div class="padmb dichvu">
                            <div class="title-sale">
                                <p><a title="Laptop bán chạy">Dịch Vụ Phòng</a></p>
                            </div>
                        </div>

                        @foreach (var item in Model.DichVus)
                        {
                            <div class="product-item" style="width: 234px;">
                                <a href="javascript:void(0)">
                                    <div class="info">
                                        <h3 style="height: unset">
                                            Tên dịch vụ: @item.TenDV
                                        </h3>
                                        <p>
                                            <span>Đơn giá</span>
                                            @item.DonGiaStr đ<span>
                                            </span>
                                        </p>
                                    </div>
                                </a>
                            </div>
                        }
                    }
                    </div>
               
                </div>
            </div>
        </section>
}
